{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3.0",
        "alexpechkarev/google-maps": "^8.0",
        "barryvdh/laravel-dompdf": "^0.8.6",
        "barryvdh/laravel-translation-manager": "^0.5.8",
        "brotzka/laravel-dotenv-editor": "^2.1",
        "bumbummen99/shoppingcart": "^2.9",
        "davejamesmiller/laravel-breadcrumbs": "^5.3",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.3",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "jeroennoten/laravel-adminlte": "^3.1",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.2",
        "laravel/legacy-factories": "^1.1",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "mews/captcha": "^3.2",
        "milon/barcode": "^8.0",
        "pusher/pusher-php-server": "^4.1",
        "renatomarinho/laravel-page-speed": "^1.8",
        "riverskies/laravel-mobile-detect": "^1.3",
        "spatie/geocoder": "^3.7",
        "spatie/laravel-activitylog": "^3.14",
        "symfony/polyfill-iconv": "^1.15"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.4",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "laracasts/generators": "^1.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Usama\\Tap\\": "packages/usama/tap/src",
            "Usama\\MyFatoorah\\": "packages/usama/myfatoorah/src",
            "Usama\\Upayment\\": "packages/usama/upayment/src"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Services/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
